<%= render 'users/user_info', user: @user %>
<%= render 'books/book_form', book: @book %>
<h1>Users</h1>

<Table>
  <tr>
	<th>image</th>
	<th>name</th>
	<th></th>
  </tr>
  <% @users. each do |user| %>
  <tr>
		<td><%= attachment_image_tag user, :profile_image %></td>
		<td><%= user.name %></td>
		<td><%= link_to "Show", user_path(user.id) %></td>
   </tr>
   <% end %>
</Table>
